# OpenRouter API configuration
OPENROUTER_API_KEY=sk-or-v1-xxx

# Model selection
DEFAULT_MODEL=google/gemini-2.5-pro
PREMIUM_MODEL=openai/gpt-5
FALLBACK_MODEL=z-ai/glm-4.5-air
GPT5_ENABLED=true
MAX_DAILY_COST=100.00

# Configuration to control model costs
ENABLE_GPT5=true
MAX_DAILY_GPT5_COST=80.00

# Service costs (per million tokens)
GPT5_INPUT_COST=0.5
GPT5_OUTPUT_COST=5.0
DEEPSEEK_INPUT_COST=0.1
DEEPSEEK_OUTPUT_COST=0.5
