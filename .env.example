# Example Environment Variables
# Copy this file to .env.local and fill in the values

# ====================
# ENVIRONMENT
# ====================
NODE_ENV=development
APP_ENV=development

# ====================
# GONG
# ====================
GONG_ACCESS_KEY=your_gong_access_key_here
GONG_CLIENT_SECRET=your_gong_client_secret_here
GONG_WEBHOOK_SECRET=your_gong_webhook_secret_here
GONG_API_URL=your_gong_api_url_here

# ====================
# N8N
# ====================
N8N_API_KEY=your_n8n_api_key_here
N8N_INSTANCE_URL=your_n8n_instance_url_here
N8N_USERNAME=your_n8n_username_here
N8N_WEBHOOK_URL=your_n8n_webhook_url_here

# ====================
# NEON
# ====================
NEON_API_TOKEN=your_neon_api_token_here
NEON_ENDPOINT=your_neon_endpoint_here
NEON_PROJECT_ID=your_neon_project_id_here
NEON_HOST=your_neon_host_here
NEON_DATABASE=your_neon_database_here
NEON_PORT=your_neon_port_here
NEON_AUTH_JWKS_URL=your_neon_auth_jwks_url_here

# ====================
# REDIS
# ====================
REDIS_USER_KEY=your_redis_user_key_here
REDIS_ACCOUNT_KEY=your_redis_account_key_here
REDIS_HOST=your_redis_host_here
REDIS_PORT=your_redis_port_here
REDIS_PASSWORD=your_redis_password_here

# ====================
# GITHUB
# ====================
GITHUB_PAT=your_github_pat_here
GITHUB_ORG=your_github_org_here

# ====================
# WEAVIATE
# ====================
WEAVIATE_URL=your_weaviate_url_here
WEAVIATE_API_KEY=your_weaviate_api_key_here

# ====================
# NETSUITE
# ====================
NETSUITE_AUTH_METHOD=your_netsuite_auth_method_here
NETSUITE_ACCOUNT_ID=your_netsuite_account_id_here
NETSUITE_APPLICATION_ID=your_netsuite_application_id_here
NETSUITE_CONSUMER_KEY=your_netsuite_consumer_key_here
NETSUITE_CONSUMER_SECRET=your_netsuite_consumer_secret_here
NETSUITE_TOKEN_ID=your_netsuite_token_id_here
NETSUITE_TOKEN_SECRET=your_netsuite_token_secret_here

# ====================
# PORTKEY
# ====================
PORTKEY_API_KEY=your_portkey_api_key_here
