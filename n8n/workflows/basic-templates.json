            "method": "POST",
            "sendBody": true,
            "bodyParameters": {
              "parameters": [
                {
                  "name": "workflow_id",
                  "value": "metrics-collection-workflow"
                },
                {
                  "name": "status",
                  "value": "completed"
                },
                {
                  "name": "metrics",
                  "value": "={{JSON.stringify($node[\"Collect Metrics\"].json)}}"
                },
                {
                  "name": "execution_id",
                  "value": "={{$executionId}}"
                },
                {
                  "name": "timestamp",
                  "value": "={{new Date().toISOString()}}"
                }
              ]
            },
            "options": {}
          },
          "name": "Completion Callback",
          "type": "n8n-nodes-base.httpRequest",
          "typeVersion": 2,
          "position": [850, 300],
