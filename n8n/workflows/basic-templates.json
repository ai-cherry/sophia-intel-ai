    {
      "id": "metrics-collection-workflow",
      "name": "Metrics Collection Workflow",
      "completion_webhook": "http://api:8003/api/nl/workflows/callback",
      "nodes": [
        {
          "parameters": {
            "httpMethod": "POST",
            "path": "collect-metrics",
            "responseMode": "onReceived",
